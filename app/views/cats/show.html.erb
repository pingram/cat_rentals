<h2><%= @cat.name %></h2>
<br />
<table>
  <tr>
    <th>Name</th>
    <td><%= @cat.name %></td>
  </tr>
  <tr>
    <th>Age</th>
    <td><%= @cat.age %></td>
  </tr>
  <tr>
    <th>Birthdate</th>
    <td><%= @cat.birth_date %></td>
  </tr>
  <tr>
    <th>Color</th>
    <td><%= @cat.color %></td>
  </tr>
  <tr>
    <th>Sex</th>
    <td><%= @cat.sex %></td>
  </tr>
</table>


<% @cat_requests.each do |cat_request| %>
    <%= cat_request.id %>
    <%= cat_request.start_date %>
    <%= cat_request.end_date %>
    <%= cat_request.status %>
    <br>
<% end %>

